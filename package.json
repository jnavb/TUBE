{
  "name": "tube-lang",
  "version": "0.2.1",
  "description": "Tube is a declarative, completely unneeded, functional language that compiles into Javascript.",
  "main": "dist/src/main.js",
  "devDependencies": {
    "@types/jest": "~26.0.20",
    "@types/node": "~14.14.31",
    "@typescript-eslint/eslint-plugin": "~4.16.1",
    "@typescript-eslint/parser": "~4.16.1",
    "eslint": "~7.21.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-plugin-jest": "~24.1.5",
    "jest": "~26.6.3",
    "prettier": "~2.2.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~26.5.2",
    "tsutils": "~3.20.0",
    "typescript": "~4.2.2"
  },
  "scripts": {
    "clean": "rimraf coverage tmp dist",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll --silent=false",
    "poc": "npm run build && node poc.js"
  },
  "dependencies": {
    "tslib": "~2.1.0",
    "tube-lang": "0.0.3"
  },
  "volta": {
    "node": "14.16.0"
  },
  "keywords": [
    "language",
    "lang",
    "functional",
    "declarative",
    "compiler"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jnavb/TUBE.git"
  },
  "homepage": "https://github.com/jnavb/TUBE.git"
}
